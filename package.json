{
  "name": "react-secure-pdf-viewer-mngo",
  "private": false,
  "version": "0.0.3",
  "sideEffects": false,
  "type": "module",
  "homepage": "./",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adityasuman2025/MNgoSecurePdfViewer.git"
  },
  "scripts": {
    "start": "vite --host",
    "build": "tsc && IS_DEPL=true vite build",
    "lib-build": "tsc && vite build",
    "build-publish": "npm run lib-build && npm publish",
    "demo": "cd demo && rm -rf node_modules && npm install && npm start"
  },
  "dependencies": {
    "react-pdf": "^7.3.3",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
